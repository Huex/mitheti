{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "core://launch/",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/out/bin/Debug/netcoreapp3.1/Mitheti.Core.dll",
            "args": [],
            "cwd": "${workspaceFolder}/out/bin/Debug/netcoreapp3.1/",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "attach://",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}
